class Geeks {
	static void getInput () {
		
	    Scanner sc = new Scanner(System.in);
	    int t=sc.nextInt(); //Taking the number of testcases
	    while(t-->0)
	    {
    		 int a =sc.nextInt() ; 
    		 System.out.println(a);
    	     String s =sc.nextLine();
    	     s += sc.nextLine();
    	     System.out.println(s);
    	     
    	    
    	    
	    }
	
	}
}
